{
    "name": "professional-rocket-simulation",
    "version": "1.3.0",
    "description": "Engineering-Grade Spaceflight Simulation",
    "type": "module",
    "scripts": {
        "build": "esbuild src/main.ts --bundle --outfile=dist/bundle.js --format=iife",
        "watch": "esbuild src/main.ts --bundle --outfile=dist/bundle.js --format=iife --watch",
        "serve": "npx http-server . -p 8080 -c-1",
        "dev": "npm run serve"
    },
    "devDependencies": {
        "typescript": "^5.3.3",
        "esbuild": "^0.19.11"
    },
    "keywords": [
        "rocket",
        "simulation",
        "physics",
        "typescript"
    ],
    "license": "MIT"
}